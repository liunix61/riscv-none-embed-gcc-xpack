{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.stablePeek": true,
  "markdownlint.config": {
    "MD041": false
  },
  "spellright.language": [
    "en_GB"
  ],
  "spellright.notificationClass": "warning",
  "cSpell.ignoreWords": [
    "binutils",
    "elfedit",
    "filt",
    "gcov",
    "gprof",
    "liquidjs",
    "objcopy",
    "objdump",
    "ranlib",
    "readelf",
    "riscv",
    "xbbla32",
    "xbbla64",
    "xbbli",
    "xbbma",
    "xbbmi"
  ]
}
